@article{mahrooRobustMultiTEASLBased2021,
  title = {Robust {{Multi-TE ASL-Based Blood}}\textendash{{Brain Barrier Integrity Measurements}}},
  author = {Mahroo, Amnah and Buck, Mareike Alicja and Huber, J{\"o}rn and Breutigam, Nora-Josefin and Mutsaerts, Henk J. M. M. and Craig, Martin and Chappell, Michael and G{\"u}nther, Matthias},
  year = {2021},
  month = dec,
  journal = {Frontiers in Neuroscience},
  volume = {15},
  pages = {719676},
  issn = {1662-453X},
  doi = {10.3389/fnins.2021.719676},
  url = {https://www.frontiersin.org/articles/10.3389/fnins.2021.719676/full},
  urldate = {2022-08-16},
  abstract = {Multiple echo-time arterial spin labelling (multi-TE ASL) offers estimation of blood\textendash tissue exchange dynamics by probing the T2 relaxation of the labelled spins. In this study, we provide a recipe for robust assessment of exchange time (Texch) as a proxy measure of blood\textendash brain barrier (BBB) integrity based on a test-retest analysis. This includes a novel scan protocol and an extension of the two-compartment model with an ``intra-voxel transit time'' (ITT) to address tissue transit effects. With the extended model, we intend to separate the underlying two distinct mechanisms of tissue transit and exchange. The performance of the extended model in comparison with the two-compartment model was evaluated in simulations. Multi-TE ASL sequence with two different bolus durations was used to acquire               in vivo               data (               n               = 10). Cerebral blood flow (CBF), arterial transit time (ATT) and Texch were fitted with the two models, and mean grey matter values were compared. Additionally, the extended model also extracted ITT parameter. The test-retest reliability of Texch was assessed for intra-session, inter-session and inter-visit pairs of measurements. Intra-class correlation coefficient (ICC) and within-subject coefficient of variance (CoV) for grey matter were computed to assess the precision of the method. Mean grey matter Texch and ITT values were found to be 227.9 {$\pm$} 37.9 ms and 310.3 {$\pm$} 52.9 ms, respectively. Texch estimated by the extended model was 32.6 {$\pm$} 5.9\% lower than the two-compartment model. A significant ICC was observed for all three measures of Texch reliability (               P               \&lt; 0.05). Texch intra-session CoV, inter-session CoV and inter-visit CoV were found to be 6.6\%, 7.9\%, and 8.4\%, respectively. With the described improvements addressing intra-voxel transit effects, multi-TE ASL shows good reproducibility as a non-invasive measure of BBB permeability. These findings offer an encouraging step forward to apply this potential BBB permeability biomarker in clinical research.},
  langid = {english},
  file = {/Users/thomaskirk/Zotero/storage/QVUI8V69/Mahroo et al. - 2021 - Robust Multi-TE ASL-Based BloodBrain Barrier Inte.pdf}
}

@article{Maier2020,
  title = {Non-Linear Fitting with Joint Spatial Regularization in Arterial Spin Labeling},
  author = {Maier, Oliver and Spann, Stefan M. and Pinter, Daniela and Gattringer, Thomas and Hinteregger, Nicole and Enzinger, Christian and Pfeuffer, Josef and Bredies, Kristian and Stollberger, Rudolf},
  year = {2020},
  journal = {arXiv},
  eprint = {2009.05409},
  eprinttype = {arxiv},
  issn = {23318422},
  abstract = {Multi-Delay single-shot arterial spin labeling (ASL) imaging provides accurate cerebral blood flow (CBF) and, in addition, arterial transit time (ATT) maps but the inherent low SNR can be challenging. Especially standard fitting using non-linear least squares often fails in regions with poor SNR, resulting in noisy estimates of the quantitative maps. State-of-the-art fitting techniques improve the SNR by incorporating prior knowledge in the estimation process which typically leads to spatial blurring. To this end, we propose a new estimation method with a joint spatial total generalized variation regularization on CBF and ATT. This joint regularization approach utilizes shared spatial features across maps to enhance sharpness and simultaneously improves noise suppression in the final estimates. The proposed method is validated in three stages, first on synthetic phantom data, including pathologies, followed by in vivo acquisitions of healthy volunteers, and finally on patient data following an ischemic stroke. The quantitative estimates are compared to two reference methods, non-linear least squares fitting and a state-of-the-art ASL quantification algorithm based on Bayesian inference. The proposed joint regularization approach outperforms the reference implementations, substantially increasing the SNR in CBF and ATT while maintaining sharpness and quantitative accuracy in the estimates. MSC Codes 92C55, 68U10, 94A12},
  archiveprefix = {arXiv},
  keywords = {Non-linear fitting,Quantitative ASL,Quantitative mapping,Stroke},
  file = {/Users/thomaskirk/Zotero/storage/7P2ZKHXB/2009.05409.pdf}
}

