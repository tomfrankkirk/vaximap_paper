@article{Greve2009,
  title = {Accurate and Robust Brain Image Alignment Using Boundary-Based Registration},
  author = {Greve, Douglas N. and Fischl, Bruce},
  year = {2009},
  month = oct,
  journal = {NeuroImage},
  volume = {48},
  number = {1},
