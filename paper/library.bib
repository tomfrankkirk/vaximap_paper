@article{LHer2014,
  title = {Bench-Test Comparison of 26 Emergency and Transport Ventilators},
  author = {L'Her, Erwan and Roy, Annie and Marjanovic, Nicolas},
  year = {2014},
  journal = {Critical Care},
  volume = {18},
