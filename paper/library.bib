@article{Cronin2020,
  title = {Mechanical {{Ventilation Redistributes Blood}} to {{Poorly Ventilated Areas}} in {{Experimental Lung Injury}}},
  author = {Cronin, John N and Crockett, Douglas C and Farmery, Andrew D and Hedenstierna, G{\"o}ran and Larsson, Anders and Camporota, Luigi and Formenti, Federico},
