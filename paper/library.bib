@article{Friston2007,
  title = {Variational Free Energy and the {{Laplace}} Approximation},
  author = {Friston, Karl and Mattout, J{\'e}r{\'e}mie and {Trujillo-Barreto}, Nelson and Ashburner, John and Penny, Will},
  year = {2007},
