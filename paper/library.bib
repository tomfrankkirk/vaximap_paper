  keywords = {Algorithms,Animal,Animals,Artificial/*methods,Disease Models,Lung Injury/*therapy,Positive-Pressure Respiration/*adverse effects,Pulmonary Atelectasis/*therapy,Pulmonary Circulation/*physiology,Respiration,Respiratory Mechanics,Swine,Tidal Volume}
}

